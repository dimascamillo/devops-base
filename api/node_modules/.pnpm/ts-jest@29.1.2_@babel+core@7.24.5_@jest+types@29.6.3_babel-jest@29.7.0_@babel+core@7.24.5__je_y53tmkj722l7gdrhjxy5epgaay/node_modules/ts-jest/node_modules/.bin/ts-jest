#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dimas/Projects/devops-basics/api/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.24.5_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.24.5__je_y53tmkj722l7gdrhjxy5epgaay/node_modules/ts-jest/node_modules:/home/dimas/Projects/devops-basics/api/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.24.5_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.24.5__je_y53tmkj722l7gdrhjxy5epgaay/node_modules:/home/dimas/Projects/devops-basics/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dimas/Projects/devops-basics/api/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.24.5_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.24.5__je_y53tmkj722l7gdrhjxy5epgaay/node_modules/ts-jest/node_modules:/home/dimas/Projects/devops-basics/api/node_modules/.pnpm/ts-jest@29.1.2_@babel+core@7.24.5_@jest+types@29.6.3_babel-jest@29.7.0_@babel+core@7.24.5__je_y53tmkj722l7gdrhjxy5epgaay/node_modules:/home/dimas/Projects/devops-basics/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
